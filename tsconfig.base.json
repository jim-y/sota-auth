{
  "compilerOptions": {
    "incremental": true,
    "target": "es2022",
    "lib": ["es2022", "esnext", "esnext.symbol", "dom"],
    "module": "commonjs",
    "declaration": true,
    "sourceMap": true,
    "strict": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "composite": true
  }
}